<body>
  <div class="parent">
    <div class="child"></div>
  </div>

  <div class="box">1</div>
  <div class="box relative">2</div>
  <div class="box">3</div>

  <div class="grand-parent">
    <div class="parent-div">
      <div class="child-div">1</div>
      <div class="child-div absolute">2</div>
      <div class="child-div">3</div>
    </div>
  </div>

</body>

